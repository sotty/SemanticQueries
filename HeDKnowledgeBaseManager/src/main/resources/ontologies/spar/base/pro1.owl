    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasIntent</IRI>
